{
  "name": "store-example",
  "version": "0.0.28",
  "private": true,
  "scripts": {
    "unsafe-dev": "next dev -p $PORT",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "gh-pages": "npm run build; npm run export; rm -rf ../store-gh-pages/*; mv ./out/* ../store-gh-pages/; rm -rf out",
    "dev": "export $(grep -v '^#' .env | xargs); npm run unsafe-dev"
  },
  "dependencies": {
    "@capacitor/core": "^2.4.7",
    "@deepcase/store": "0.0.34",
    "next": "^10.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.2"
  },
  "engines": {
    "node": "^14.15.0"
  }
}
